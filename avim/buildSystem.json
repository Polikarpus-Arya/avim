{
	"py":
	{
		"Name"		:"Python",
		"Build"		:"py $filePath/$fileName",
		"Run"		:"py $filePath/$fileName",
		"Extension"	:"py"
	},
	"cpp":
	{
		"Name"		:"Cplusplus",
		"Build"		:"g++ -std=c++17 -O2 -o $filePath/$fileBaseName $filePath/$fileName",
		"Run"		:"$filePath/$fileBaseName",
		"Extension"	:"cpp"
	},
	"c":
	{
		"Name"		:"C",
		"Build"		:"gcc -o $filePath/$fileBaseName $filePath/$fileName",
		"Run"		:"$filePath/$fileBaseName",
		"Extension"	:"c"
	},
	"java":
	{
		"Name"		: "Java",
		"Build"		: "javac $filePath/$fileName",
		"Run"		: "java $filePath/$fileBaseName",
		"Extension"	: "java"
	}
}
